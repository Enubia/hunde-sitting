        "lint": "eslint .",
        "start": "node dist/index.js",
        "check-types": "tsc --noEmit"
    },
    "dependencies": {
        "@hono/node-server": "1.13.8",
        "hono": "4.7.4",
        "stoker": "1.4.2",
        "zod": "3.24.2"
    },
    "devDependencies": {
        "@hunde-sitting/eslint-config": "workspace:*",
        "@types/node": "22.13.9",
        "tsc-alias": "1.8.11",
        "tscw-config": "^1.1.2",
        "tsx": "4.19.3",
        "typescript": "5.8.2"
    }
}
