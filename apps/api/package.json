{
    "name": "@hunde-sitting/api",
    "type": "module",
    "version": "1.0.0",
    "exports": {
        "./routes": {
            "types": "./src/routes/index.ts",
            "default": "./dist/routes/index.js"
        }
    },
    "scripts": {
        "build": "tsc && tsc-alias",
        "clean": "rm -rf dist && rm -rf .turbo",
